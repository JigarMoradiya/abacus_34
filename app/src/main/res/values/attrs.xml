<resources>
    <!-- Abacus master view-->
    <declare-styleable name="AbacusView">
        <attr name="beadDrawables" format="reference" />
        <attr name="noOfRows" format="integer" />
        <attr name="noOfBead" format="integer" />
        <attr name="singleBeadValue" format="integer" />
